
{% extends 'base.html.twig' %}


        {% block body %}
        {% if articles %}
            <table class="table table-striped" id="table">
              <thead>
                <tr>
                    <th>Article title</th>
                </tr>
              </thead>
                <tbody>
                   {% for article in articles %}
                      <tr> <td>{{ article.title }}</td></tr>
                      
                  {% endfor %}
                </tbody>
        </table>
           {% else %}
           <p> No Articles to display <p>
           {% endif %}
          {% endblock %}


        {% block javascripts %} <script src="/js/main.js"></script> {% endblock %}